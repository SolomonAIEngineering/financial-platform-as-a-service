    "stripe": "^16.12.0",
    "typesense": "^1.8.2",
    "xior": "^0.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@cloudflare/vitest-pool-workers": "^0.4.24",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@types/bun": "^1.1.9",
